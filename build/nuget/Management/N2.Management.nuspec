<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>N2CMS.Management</id>
		<version>{Version}</version>
		<title>N2 CMS Management</title>
		<authors>Cristian Libardo</authors>
		<owners>Cristian Libardo</owners>
		<licenseUrl>http://n2cms.com/Project/License.aspx</licenseUrl>
		<projectUrl>http://n2cms.com/</projectUrl>
		<iconUrl>http://n2cms.com/Upload/logo-5.png</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>This package contains the management interface required by N2 CMS. This package doesn't contain templates. N2 CMS is a lightweight CMS library that can be used as is, or in combination with many features out of the box using template pack for MVC2-4, Razor or WebForms.</description>
		<summary></summary>
		<language>en-US</language>
		<tags>N2 CMS</tags>
		<dependencies>
			<dependency id="N2CMS.Library" version="{Version}" />
			<dependency id="Lucene.Net" version="3.0" />
			<!--<dependency id="SqlServerCompact" version="4.0.8482.1"/>-->
			<dependency id="DotNetZip" version="1.9.1.8" />
		</dependencies>
	</metadata>
  <files>
    <file src="..\..\build\NuGet\Management\**\*.*" target="." />
    <file src="content\N2\N2.zip" target="content\N2\" />
    <file src="content\N2\web.config" target="content\N2\" />
    <file src="..\..\src\Mvc\MvcTemplates\bin\N2.Management.*" target="lib\net20" />
    <file src="..\..\src\Mvc\MvcTemplates\**\*.cs" target="src\Mvc\MvcTemplates" />
    <file src="..\..\src\Framework\Extensions\bin\Release\N2.Extensions.*" target="lib\net20" />
    <file src="..\..\src\Framework\Extensions\**\*.cs" target="src\Framework\Extensions" />
    <file src="..\..\src\Framework\Packaging\bin\Release\N2.Packaging.dll" target="lib\net20" />
    <file src="..\..\src\Framework\Packaging\**\*.cs" target="src\Framework\Packaging" />
  </files>
</package>
